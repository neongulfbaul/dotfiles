# The zplug cache directory.
# See:
#   1. https://github.com/zplug/zplug#zplug_cache_dir
export ZPLUG_CACHE_DIR=$IGLOO_PATH_USER_CACHE/zsh/zplug

# The home directory to store/load installed plugins/packages.
# See:
#   1. https://github.com/zplug/zplug#zplug_home
export ZPLUG_HOME=$IGLOO_PATH_USER_DATA/zsh/zplug
